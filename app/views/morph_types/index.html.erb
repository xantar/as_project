<% title "Morph Types" %>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for morph_type in @morph_types %>
    <tr>
      <td><%= link_to(morph_type.name, morph_type) %></td>
      <td><%= link_to "Edit", edit_morph_type_path(morph_type) %></td>
      <td><%= link_to "Destroy", morph_type, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Morph Type", new_morph_type_path %></p>
