<%= form_for @dragon do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :number %><br />
    <%= f.text_field :number %>
  </p>
  <p>
    <%= f.label :clutch_id %><br />
    <%= f.number_field :clutch_id %>
  </p>
  <p>
    <%= f.label :source_id %><br />
    <%= f.number_field :source_id %>
  </p>
  <p>
    <%= f.label :group_id %><br />
    <%= f.number_field :group_id %>
  </p>
  <p>
    <%= f.label :status_id %><br />
    <%= f.number_field :status_id %>
  </p>
  <p>
    <%= f.label :male %><br />
    <%= f.check_box :male %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
