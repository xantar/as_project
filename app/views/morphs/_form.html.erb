<%= form_for @morph do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :dragon_id %><br />
    <%= f.number_field :dragon_id %>
  </p>
  <p>
    <%= f.label :morph_type %><br />
    <%= f.number_field :morph_type %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
